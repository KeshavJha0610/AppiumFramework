            <include name="Smoke"/>
        </run>
    </groups>

    <listeners>
        <listener class-name="org.academy.TestUtils.Listeners"/>
    </listeners>

    <test thread-count="5" name="End to end Framework Test">
        <classes>
            <class name="org.academy.Ecommerce4Hybrid"/>
        </classes>
    </test> <!-- Test -->

    <test thread-count="5" name="Error Validations Test">
        <classes>
            <class name="org.academy.Ecommerce2"/>
        </classes>
    </test> <!-- Test -->

   <!-- <test thread-count="5" name="IOS Test">
